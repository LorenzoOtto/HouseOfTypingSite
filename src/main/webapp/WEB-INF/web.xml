<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>houseoftyping</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>CourseController</servlet-name>
    <servlet-class>houseoftyping.controller.CourseController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CourseController</servlet-name>
    <url-pattern>/CourseController</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FormController</servlet-name>
    <servlet-class>houseoftyping.controller.FormController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FormController</servlet-name>
    <url-pattern>/FormController</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginController</servlet-name>
    <servlet-class>houseoftyping.controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginController</servlet-name>
    <url-pattern>/LoginController</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ExcelController</servlet-name>
    <servlet-class>houseoftyping.controller.ExcelController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ExcelController</servlet-name>
    <url-pattern>/ExcelController</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>MollieController</servlet-name>
    <servlet-class>houseoftyping.controller.MollieController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MollieController</servlet-name>
    <url-pattern>/MollieController</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>login</servlet-name>
    <jsp-file>/login.html</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>dashboard</servlet-name>
    <jsp-file>/dashboard.html</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>dashboard</servlet-name>
    <url-pattern>/dashboard</url-pattern>
  </servlet-mapping>
</web-app>